{
  "name": "wamschat",
  "version": "1.0.0",
  "description": "Client-side chat application using AMQP streams over WebSockets",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:with-mock": "concurrently \"node mock-amqp-server.js\" \"vite\"",
    "mock-server": "node mock-amqp-server.js",
    "test-consumer": "node test-consumer.js",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write src/"
  },
  "keywords": [
    "chat",
    "amqp",
    "websocket",
    "streams",
    "lavinmq"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@vitest/ui": "^2.0.0",
    "eslint": "^9.0.0",
    "globals": "^15.0.0",
    "prettier": "^3.0.0",
    "vite": "^5.0.0",
    "vitest": "^2.0.0"
  },
  "dependencies": {
    "@cloudamqp/amqp-client": "file:../amqp-client.js",
    "concurrently": "^9.0.0",
    "jsdom": "^23.0.0",
    "ws": "^8.18.0"
  }
}
